description <http://example.org/test-property-values#> as test-property-values {
  
  uses <http://www.w3.org/2001/XMLSchema#> as xsd

	uses <http://example.org/some-domain#> as some-domain
  
  // Note: Unique Name Assumption: 'camera1' would be insufficiently unique, see test-metrology-model.oml
  instance camera1 : some-domain:Component 
  [ some-domain:name "test-property-values/camera1" 
    some-domain:mass.basic "0.31"^^xsd:double
    some-domain:mass.allocation "0.5"^^xsd:double ]
  
  // Note: Unique Name Assumption: 'camera2' would be insufficiently unique, see test-metrology-model.oml
  instance camera2 : some-domain:Component
  [ some-domain:name "test-property-values/camera2" 
    some-domain:mass.basic "0.5"^^xsd:double
    some-domain:mass.allocation "0.8"^^xsd:double ] 

}