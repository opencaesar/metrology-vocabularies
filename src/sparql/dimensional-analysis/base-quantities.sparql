PREFIX vim4:  <http://bipm.org/jcgm/vim4#>
PREFIX oml:   <http://opencaesar.io/oml#>

SELECT DISTINCT ?sq ?bq ?d
WHERE {
  ?sq 
    a vim4:SystemOfQuantities .
  
  ?sbq
    a vim4:SystemHasBaseQuantity ;
    oml:hasSource ?sq ;
    oml:hasTarget ?bq ;
    vim4:dimensionSymbol ?d .

}
ORDER BY ?sq ?bq
